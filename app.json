{
  "expo": {
    "name": "INRA APP",
    "slug": "applicationGDT",
    "version": "2.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.applicationgdt.INRA_APP",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ],
      "versionCode": 1,
      "enableProguardInReleaseBuilds": true,
      "resConfigs": [
        "en",
        "fr",
        "hdpi",
        "xhdpi"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/adaptive-icon.png",
      "output": "single",
      "backgroundColor": "#ffffff",
      "description": "INRA timesheet",
      "display": "standalone",
      "themeColor": "#ffffff",
      "start_url": "/",
      "scope": "/",
      "name": "INRA timesheet",
      "shortName": "INRA timesheet",
      "categories": [
        "business",
        "productivity"
      ],
      "lang": "fr",
      "orientation": "portrait",
      "icons": [
        {
          "src": "./assets/images/icon.png",
          "sizes": "192x192",
          "type": "image/png",
          "purpose": "any maskable"
        },
        {
          "src": "./assets/images/adaptive-icon.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "any maskable"
        }
      ],
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-file-system",
        {
          "fileSystemSchemes": [
            "file",
            "content"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eff49df0-c731-45c0-86e2-091f7a917e71"
      }
    },
    "owner": "oussama_fnh"
  }
}